    <!-- Gingerbread to Jelly Bean -->
    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />

    <!-- Used for caching and creating new playlists -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
